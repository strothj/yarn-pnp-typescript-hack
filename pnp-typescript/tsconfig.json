{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "../kitchen-sink/resolvers/typescript/lib",
    "rootDir": "./",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  },
  "include": ["./src/**/*", "./util/**/*"]
}
